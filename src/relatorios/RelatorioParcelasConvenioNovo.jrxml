	</variable>
	<variable name="comissaoValor" class="java.lang.Double">
		<variableExpression><![CDATA[$V{baseDescontoTotal} * $V{comissao} * 0.01]]></variableExpression>
	</variable>
	<variable name="totalConvenioAcrescimo" class="java.lang.Double" calculation="Sum">
