	<variable name="pago" class="java.lang.Boolean">
		<variableExpression><![CDATA[$F{parcela_pago}]]></variableExpression>
	</variable>
	<variable name="parcela" class="java.lang.Double">
		<variableExpression><![CDATA[$F{parcela_valorparcela}.doubleValue()]]></variableExpression>
	</variable>
	<variable name="valorPago" class="java.lang.Double">
		<variableExpression><![CDATA[( $V{pago} ? $V{parcela} : 0.0 )]]></variableExpression>
	</variable>
	<variable name="acrescimo" class="java.lang.Double">
		<variableExpression><![CDATA[( $V{pago} ? 0.0 : 
    ($F{parcela_vencimento}.before($P{PERIODO}) ? $V{parcela} * 0.05 : 0.0 ))]]></variableExpression>
	</variable>
	<variable name="baseDescontoCliente" class="java.lang.Double">
		<variableExpression><![CDATA[($V{acrescimo} > 0 ? 0.0 : $V{parcela})]]></variableExpression>
	</variable>
	<variable name="saldoParcial" class="java.lang.Double">
		<variableExpression><![CDATA[( $V{pago} ? 0.0 : $V{parcela}.doubleValue() + $V{acrescimo}.doubleValue())]]></variableExpression>
	</variable>
