	</variable>
	<variable name="totalSaldoConvenio" class="java.lang.Double">
		<variableExpression><![CDATA[$V{totalConvenio} - $V{totalConvenioPago}]]></variableExpression>
	</variable>
	<variable name="comissao" class="java.lang.Double">
