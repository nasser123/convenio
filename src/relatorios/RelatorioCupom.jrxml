	<parameter name="IDVENDA" class="java.lang.Integer"/>
	<parameter name="EMPRESA" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
     venda.`idvenda` AS venda_idvenda,
     venda.`data` AS venda_data,
     venda.`nrparcelas` AS venda_nrparcelas,
     venda.`notafiscal` AS venda_notafiscal,
     venda.`valor` AS venda_valor,
     venda.`idcliente` AS venda_idcliente,
     parcela.`idparcela` AS parcela_idparcela,
     parcela.`nrparcela` AS parcela_nrparcela,
     parcela.`idvenda` AS parcela_idvenda,
     parcela.`valorparcela` AS parcela_valorparcela,
     parcela.`vencimento` AS parcela_vencimento,
     parcela.`pago` AS parcela_pago,
     Empresa.`idEmpresa` AS Empresa_idEmpresa,
     Empresa.`razaosocial` AS Empresa_razaosocial,
     Empresa.`fantasia` AS Empresa_fantasia,
     Empresa.`cnpj` AS Empresa_cnpj,
     Empresa.`endereco` AS Empresa_endereco,
     Empresa.`telefone` AS Empresa_telefone,
     Empresa.`cidade` AS Empresa_cidade,
     cliente.`idcliente` AS cliente_idcliente,
     cliente.`idconvenio` AS cliente_idconvenio,
     cliente.`idsexo` AS cliente_idsexo,
     cliente.`idestadocivil` AS cliente_idestadocivil,
     cliente.`nome` AS cliente_nome,
     cliente.`telefone` AS cliente_telefone,
     cliente.`endereco` AS cliente_endereco,
     cliente.`endereconr` AS cliente_endereconr,
     cliente.`bairro` AS cliente_bairro,
     cliente.`cpf` AS cliente_cpf,
     cliente.`identidade` AS cliente_identidade,
     cliente.`naturalidade` AS cliente_naturalidade,
     cliente.`pai` AS cliente_pai,
     cliente.`mae` AS cliente_mae,
     cliente.`emprego` AS cliente_emprego,
     cliente.`cargo` AS cliente_cargo,
     cliente.`dataadmissao` AS cliente_dataadmissao,
     cliente.`renda` AS cliente_renda,
     cliente.`datanasc` AS cliente_datanasc,
     cliente.`limite` AS cliente_limite,
     cliente.`situacao` AS cliente_situacao,
     cliente.`observacao` AS cliente_observacao,
     convenio.`idconvenio` AS convenio_idconvenio,
     convenio.`nome` AS convenio_nome,
     convenio.`responsavel` AS convenio_responsavel,
     convenio.`telefone` AS convenio_telefone,
     convenio.`situacao` AS convenio_situacao,
     convenio.`nrparcelas` AS convenio_nrparcelas,
     convenio.`vencimento` AS convenio_vencimento,
     convenio.`datalimite` AS convenio_datalimite
FROM
     `venda` venda INNER JOIN `parcela` parcela ON venda.`idvenda` = parcela.`idvenda`
     INNER JOIN `cliente` cliente ON venda.`idcliente` = cliente.`idcliente`
     INNER JOIN `convenio` convenio ON cliente.`idconvenio` = convenio.`idconvenio`,
     `Empresa` Empresa
WHERE
     venda.`idvenda` = $P{IDVENDA}
 AND Empresa.`idEmpresa` = $P{EMPRESA}
ORDER BY
     parcela.`idparcela` ASC]]>
	</queryString>
