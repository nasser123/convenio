    }//GEN-LAST:event_jCheckBoxClientesActionPerformed

    private void jComboBoxConveniosActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jComboBoxConveniosActionPerformed
        this.c = (Convenio) jComboBoxConvenios.getSelectedItem();

        clienteQuery = convenioPUEntityManager.createNamedQuery("Cliente.findByIdconvenio");
        clienteQuery.setParameter("idconvenio", this.c);

        java.util.Collection data = clienteQuery.getResultList();

        clienteList.clear();
        clienteList.addAll(data);



    }//GEN-LAST:event_jComboBoxConveniosActionPerformed

    private void jButtonRelatorioActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButtonRelatorioActionPerformed
        ExecutaRelatorio t = new ExecutaRelatorio();
        Convenio c = (Convenio) jComboBoxConvenios.getSelectedItem();
        Integer mes = jMonthChooser1.getMonth() + 1;
        Integer ano = jYearChooser1.getYear();


        if (jRadioButtonVendas.isSelected()) {

            t.abrirRelatorioClientes("RelatorioVenda.jasper");

        } else if (jRadioButtonParcelasConvenio.isSelected()) {
            HashMap<String, Object> parameterMap = new HashMap<String, Object>();

            Object convenio = c.getIdconvenio();
            Object periodo = Datas.getData("10/" + mes.toString() + "/" + ano.toString());

            t.abrirRelatorioParcelasConvenio(convenio, periodo);


        } else {


            t.abrirRelatorioClientes("RelatorioParcelas.jasper");

        }
    }//GEN-LAST:event_jButtonRelatorioActionPerformed

    private void organizaTela() {
        if (jRadioButtonVendas.isSelected()) {
            jPanelPeriodoDatas.setVisible(true);
            jPanelPeriodoMes.setVisible(false);
        }
        if (jRadioButtonParcelasConvenio.isSelected()) {
            jPanelPeriodoDatas.setVisible(false);
            jPanelPeriodoMes.setVisible(true);

        }
         if (jRadioButtonVendasConvenio.isSelected()) {
            jPanelPeriodoDatas.setVisible(true);
            jPanelPeriodoMes.setVisible(false);

        }

    }

