    }//GEN-LAST:event_jRadioButtonParcelasConvenioActionPerformed

    private void jCheckBoxClientesActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jCheckBoxClientesActionPerformed
