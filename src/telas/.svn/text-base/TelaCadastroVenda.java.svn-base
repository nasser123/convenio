    private Date calculaVencimento(Date dVenda, int prestacao, boolean pm) {
        Date data = dVenda;
        Date dataDesconto = null;
        boolean proxMes = pm;
        int nPrestacao = prestacao;
        Integer nDiaVenda = Datas.getDia(data);
        Integer nMesVenda = Datas.getMes(data);
        Integer nAnoVenda = Datas.getAno(data);
        int nDiaDesconto = 10;
        int nMesDesconto = 0;
        int nAnoDesconto = nAnoVenda;
        int nProxMes = 0;

        if (proxMes == true) {
            nProxMes = 1;
        }

        if (nDiaVenda < 23) {
            nMesDesconto = nMesVenda + nPrestacao + nProxMes;
        } else {
            nMesDesconto = nMesVenda + nPrestacao + 1 + nProxMes;
        }

        if (nMesDesconto > 12) {
            nMesDesconto = nMesDesconto - 12;
            nAnoDesconto++;
        }

        String sDataDesconto = nDiaDesconto + "/" + nMesDesconto + "/" + nAnoDesconto;
        System.out.println(sDataDesconto);
        dataDesconto = Datas.transformaStringDate(sDataDesconto);
        return dataDesconto;

    }
    private void jButtonGravarActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButtonGravarActionPerformed
        Integer nrParcelas = jComboBox1.getSelectedIndex() + 1;
        ArrayList<Float> parcelas = new ArrayList();
        this.validado = true;
        boolean proxMes = jCheckBoxProximoMes.isSelected();

        Venda v = new Venda();

        verificaNotaFiscal();
        verificaValor();

        if (validado == true) {
            parcelas = ValidarValores.calculaParcelas(valor, nrParcelas);
            v.setIdcliente(this.c);
            v.setData(jDateChooserData.getDate());
            v.setNotafiscal(Integer.parseInt(jTextFieldNotaFiscal.getText()));
            v.setNrparcelas(nrParcelas);
            v.setValor(valor);

            if (!entityManager.getTransaction().isActive()) {
                entityManager.getTransaction().begin();
            }
            entityManager.persist(v);
//            entityManager.getTransaction().commit();

//            entityManager.getTransaction().begin();
            for (int i = 0; i < parcelas.size(); i++) {

                Parcela p = new Parcela();
                p.setIdvenda(v);
                // p.setIdparcela(i + 1);
                p.setValorparcela(parcelas.get(i));
                p.setNrparcela(i + 1);
                p.setVencimento(calculaVencimento(v.getData(), (i + 1), proxMes));
                entityManager.persist(p);

            }
            entityManager.getTransaction().commit();

            JOptionPane.showMessageDialog(rootPane, "Venda cadastrada com sucesso!");

            TelaCadastroVenda tcv = new TelaCadastroVenda();
            tcv.setVisible(true);
            this.dispose();
        } else {
            JOptionPane.showMessageDialog(rootPane, "Verifique os dados digitados");

        }

    }//GEN-LAST:event_jButtonGravarActionPerformed

