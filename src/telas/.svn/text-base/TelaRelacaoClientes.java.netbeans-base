    private void jButtonEditarActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButtonEditarActionPerformed
        Cliente c = new Cliente();
        c = (Cliente) jComboBoxSelecionado.getSelectedItem();
        TelaCadastroCliente tcc = new TelaCadastroCliente(c);
        tcc.setVisible(true);
        this.dispose();
    }//GEN-LAST:event_jButtonEditarActionPerformed

    private void jButtonExcluirActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButtonExcluirActionPerformed
        int teste = JOptionPane.showConfirmDialog(rootPane, "Você tem certeza que deseja excluir o cliente?");
        if (teste == 0) {
            Cliente c = (Cliente) jComboBoxSelecionado.getSelectedItem();
            if (c.getVendaList().size() > 0) {
                JOptionPane.showMessageDialog(rootPane, "Existem vendas cadastradas para esse cliente. Você não pode excluir");
            } else {
                if (!convenioPUEntityManager.getTransaction().isActive()) {
                    convenioPUEntityManager.getTransaction().begin();
                }
                convenioPUEntityManager.remove(c);
                convenioPUEntityManager.getTransaction().commit();
                JOptionPane.showMessageDialog(rootPane, "Cliente excluido com sucesso");
            }

        }

    }//GEN-LAST:event_jButtonExcluirActionPerformed

    private void jButtonNovoActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButtonNovoActionPerformed
