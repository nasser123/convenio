    private void jButtonExcluirActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButtonExcluirActionPerformed
        if (jComboBox1.getSelectedItem() != null) {
            Venda v = (Venda) jComboBox1.getSelectedItem();
            if (!convenioPUEntityManager.getTransaction().isActive()) {
                convenioPUEntityManager.getTransaction().begin();
            }
            convenioPUEntityManager.remove(v);
            convenioPUEntityManager.getTransaction().commit();
        }
        this.atualizaTabelas(evt);


    }//GEN-LAST:event_jButtonExcluirActionPerformed

    private void atualizaTabelas(java.awt.event.ActionEvent evt) {
        convenioPUEntityManager.getTransaction().rollback();
        if (!convenioPUEntityManager.getTransaction().isActive()) {
            convenioPUEntityManager.getTransaction().begin();
        }
        java.util.Collection data = vendaQuery.getResultList();
        for (Object entity : data) {
            convenioPUEntityManager.refresh(entity);
        }
        vendaList.clear();
        vendaList.addAll(data);
    }

    private void verificaCliente() {
        Integer id = 0;
        boolean validado = true;
        if (jTextFieldCodigo.getText() != null) {
            try {
                id = Integer.parseInt(jTextFieldCodigo.getText());
                queryCliente = convenioPUEntityManager.createNamedQuery("Cliente.findByIdcliente");

                queryCliente.setParameter("idcliente", id);
                listCliente = queryCliente.getResultList();
                this.c = (Cliente) listCliente.get(0);

                preencheDados();
                filtrar(this.c);

            } catch (NumberFormatException nfe) {
                //JOptionPane.showMessageDialog(rootPane, "Digite apenas números!");
                jTextFieldCodigo.setText(null);
                validado = false;

            } catch (IndexOutOfBoundsException iobe) {
                JOptionPane.showMessageDialog(rootPane, "Cliente não encontrado!");
                validado = false;
            }

        }

    }

    private void preencheDados() {
        jTextFieldCodigo.setText(this.c.getIdcliente().toString());
        jTextFieldNome.setText(this.c.getNome());

    }

