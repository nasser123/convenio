    public void filtrar(Cliente c) {
        this.c = c;

        vendaQuery = convenioPUEntityManager.createNamedQuery("Venda.findByIdcliente");
        vendaQuery.setParameter("idcliente", this.c);

        java.util.Collection data = vendaQuery.getResultList();

        vendaList.clear();
        vendaList.addAll(data);

        if (vendaList.size() != 0) {
            jTableVendas.setRowSelectionInterval(0, 0);
        }
    }

