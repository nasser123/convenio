    private void preencheCampos() {
        jTextFieldCodigo.setEnabled(false);
        jComboBoxConvenio.setSelectedItem(this.c.getIdconvenio());
        jComboBoxSexo.setSelectedItem(this.c.getIdsexo());
        jComboBoxEstadoCivil.setSelectedItem(this.c.getIdestadocivil());
        jTextFieldCodigo.setText(this.c.getIdcliente().toString());

        if (this.c.getNome() != null) {
            jTextFieldNome.setText(this.c.getNome());
        }

        if (this.c.getTelefone() != null) {
            jTextFieldTelefone.setText(this.c.getTelefone());
        }

        if (this.c.getEndereco() != null) {
            jTextFieldEndereco.setText(this.c.getEndereco());
        }

        if (this.c.getEndereconr() != null) {
            jTextFieldEnderecoNr.setText(this.c.getEndereconr());
        }

        if (this.c.getBairro() != null) {
            jTextFieldBairro.setText(this.c.getBairro());
        }

        if (this.c.getCpf() != null) {
            jTextFieldCPF.setText(this.c.getCpf());
        }

        if (this.c.getIdentidade() != null) {
            jTextFieldRG.setText(this.c.getIdentidade());
        }

        if (this.c.getNaturalidade() != null) {
            jTextFieldNaturalidade.setText(this.c.getNaturalidade());
        }

        if (this.c.getPai() != null) {
            jTextFieldNomePai.setText(this.c.getPai());
        }

        if (this.c.getMae() != null) {
            jTextFieldNomeMae.setText(this.c.getMae());
        }

        if (this.c.getEmprego() != null) {
            jTextFieldEmprego.setText(this.c.getEmprego());
        }

        if (this.c.getCargo() != null) {
            jTextFieldCargo.setText(this.c.getCargo());
        }

        if (this.c.getDataadmissao() != null) {
            jDateChooserAdmissao.setDate(this.c.getDataadmissao());
        }

        if (this.c.getRenda() != null) {
            jFormattedTextFieldRenda.setText(this.c.getRenda().toString());
        }

        if (this.c.getLimite() != null) {
            jFormattedTextFieldLimite.setText(this.c.getLimite().toString());
        }

        if (this.c.getDatanasc() != null) {
            jDateChooser1.setDate(this.c.getDatanasc());
        }

        jTextAreaObservacao.setText(this.c.getObservacao());
        jComboBoxSituacao.setSelectedItem(this.c.getSituacao());

    }

